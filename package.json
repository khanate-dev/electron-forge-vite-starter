		"eslint-plugin-vitest": "^0.3.2",
		"jsdom": "^24.0.0",
		"msw": "^2.2.14",
		"node-html-parser": "^6.1.13",
		"prettier": "^3.2.5",
		"react-refresh": "^0.14.2",
		"svgo": "^3.2.0",
		"ts-node": "^10.9.2",
		"tsx": "^4.7.3",
		"typescript": "~5.4.5",
		"vite": "^5.2.10",
		"vite-plugin-eslint": "^1.8.1",
