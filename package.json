{
	"name": "electron-starter",
	"productName": "electron-starter",
	"version": "1.0.0",
	"description": "Electron React Starter Application",
	"keywords": [],
	"author": {
		"name": "Khanate",
		"email": "dev@khanate.pk",
		"url": "https://khante.pk"
	},
	"main": ".vite/build/main.js",
	"scripts": {
		"start": "cross-env DEV=1 electron-forge start",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "electron-forge publish",
		"test": "vitest watch",
		"test-ui": "vitest --ui",
		"typecheck": "vitest typecheck",
		"lint": "eslint . --fix --report-unused-disable-directives && pnpm format && pnpm spellcheck",
		"format": "prettier --write --log-level silent .",
		"spellcheck": "cspell . --quiet",
		"generate-icons": "tsx ./scripts/generate-icons.ts",
		"generate-favicons": "electron-icon-builder --input=./src/app/assets/favicon/favicon.png --output=./src/app/assets/favicon",
		"audit": "electronegativity -i ./",
		"diag": "tsc --extendedDiagnostics",
		"trace": "tsc --generateTrace ./trace && npx @typescript/analyze-trace trace"
	},
	"dependencies": {
		"@emotion/react": "^11.11.4",
		"@emotion/styled": "^11.11.5",
		"@mui/icons-material": "^5.15.15",
		"@mui/material": "^5.15.15",
		"@mui/x-date-pickers": "^7.3.1",
		"dayjs": "^1.11.10",
		"electron-squirrel-startup": "^1.0.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-hot-toast": "^2.4.1",
		"react-router-dom": "^6.23.0",
		"serialport": "^12.0.0",
		"xlsx": "^0.18.5",
		"zod": "^3.23.4"
	},
	"devDependencies": {
		"@doyensec/electronegativity": "^1.10.3",
		"@electron-forge/cli": "^7.4.0",
		"@electron-forge/maker-deb": "^7.4.0",
		"@electron-forge/maker-squirrel": "^7.4.0",
		"@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
		"@electron-forge/plugin-fuses": "^7.4.0",
		"@electron-forge/plugin-vite": "^7.4.0",
		"@electron-forge/publisher-github": "^7.4.0",
		"@mui/system": "^5.15.15",
		"@testing-library/jest-dom": "^6.4.2",
		"@testing-library/react": "^15.0.5",
		"@testing-library/user-event": "^14.5.2",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/eslint": "^8.56.10",
		"@types/node": "^20.12.7",
		"@types/prettier": "^3.0.0",
		"@types/react": "^18.3.1",
		"@types/react-dom": "^18.3.0",
		"@types/react-router-dom": "^5.3.3",
		"@types/testing-library__jest-dom": "^6.0.0",
		"@typescript-eslint/eslint-plugin": "^6.8.0",
		"@typescript-eslint/parser": "^6.8.0",
		"@typescript/analyze-trace": "^0.10.1",
		"@vitejs/plugin-react": "^4.2.1",
		"@vitest/ui": "^1.5.2",
		"cross-env": "^7.0.3",
		"cspell": "^8.7.0",
		"electron": "30.0.1",
		"electron-devtools-assembler": "^1.2.0",
		"electron-icon-builder": "^2.0.1",
		"eslint": "^8.51.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.28.1",
